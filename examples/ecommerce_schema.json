{
  "project_name": "EcommerceBackend",
  "db_type": "postgresql",
  "description": "E-commerce backend with Products, Orders, and Inventory",
  "schema": [
    {
      "table_name": "users",
      "class_name": "User",
      "options": {
        "use_timestamps": true,
        "use_soft_delete": false
      },
      "columns": [
        {
          "name": "id",
          "type": "Integer",
          "primary_key": true
        },
        {
          "name": "email",
          "type": "String",
          "length": 100,
          "unique": true,
          "nullable": false
        },
        {
          "name": "username",
          "type": "String",
          "length": 50,
          "unique": true,
          "nullable": false
        },
        {
          "name": "role",
          "type": "Enum",
          "values": ["customer", "admin"],
          "default": "customer"
        }
      ],
      "relationships": [
        {
          "target_table": "orders",
          "target_class": "Order",
          "type": "one_to_many",
          "back_populates": "user"
        }
      ]
    },
    {
      "table_name": "categories",
      "class_name": "Category",
      "options": {
        "use_timestamps": true,
        "use_soft_delete": false
      },
      "columns": [
        {
          "name": "id",
          "type": "Integer",
          "primary_key": true
        },
        {
          "name": "name",
          "type": "String",
          "length": 100,
          "unique": true,
          "nullable": false
        },
        {
          "name": "description",
          "type": "Text",
          "nullable": true
        }
      ],
      "relationships": [
        {
          "target_table": "products",
          "target_class": "Product",
          "type": "one_to_many",
          "back_populates": "category"
        }
      ]
    },
    {
      "table_name": "products",
      "class_name": "Product",
      "options": {
        "use_timestamps": true,
        "use_soft_delete": true
      },
      "columns": [
        {
          "name": "id",
          "type": "Integer",
          "primary_key": true
        },
        {
          "name": "name",
          "type": "String",
          "length": 200,
          "nullable": false
        },
        {
          "name": "description",
          "type": "Text",
          "nullable": true
        },
        {
          "name": "price",
          "type": "Numeric",
          "precision": 10,
          "scale": 2,
          "nullable": false
        },
        {
          "name": "sku",
          "type": "String",
          "length": 50,
          "unique": true,
          "nullable": false
        },
        {
          "name": "category_id",
          "type": "ForeignKey",
          "target": "categories.id"
        }
      ],
      "relationships": [
        {
          "target_table": "categories",
          "target_class": "Category",
          "type": "many_to_one",
          "back_populates": "products"
        },
        {
          "target_table": "order_items",
          "target_class": "OrderItem",
          "type": "one_to_many",
          "back_populates": "product"
        }
      ]
    },
    {
      "table_name": "orders",
      "class_name": "Order",
      "options": {
        "use_timestamps": true,
        "use_soft_delete": false
      },
      "columns": [
        {
          "name": "id",
          "type": "Integer",
          "primary_key": true
        },
        {
          "name": "order_number",
          "type": "String",
          "length": 50,
          "unique": true,
          "nullable": false
        },
        {
          "name": "status",
          "type": "Enum",
          "values": ["pending", "processing", "shipped", "delivered", "cancelled"],
          "default": "pending"
        },
        {
          "name": "total_amount",
          "type": "Numeric",
          "precision": 10,
          "scale": 2,
          "nullable": false
        },
        {
          "name": "user_id",
          "type": "ForeignKey",
          "target": "users.id"
        }
      ],
      "relationships": [
        {
          "target_table": "users",
          "target_class": "User",
          "type": "many_to_one",
          "back_populates": "orders"
        },
        {
          "target_table": "order_items",
          "target_class": "OrderItem",
          "type": "one_to_many",
          "back_populates": "order"
        }
      ]
    },
    {
      "table_name": "order_items",
      "class_name": "OrderItem",
      "options": {
        "use_timestamps": true,
        "use_soft_delete": false
      },
      "columns": [
        {
          "name": "id",
          "type": "Integer",
          "primary_key": true
        },
        {
          "name": "quantity",
          "type": "Integer",
          "nullable": false
        },
        {
          "name": "unit_price",
          "type": "Numeric",
          "precision": 10,
          "scale": 2,
          "nullable": false
        },
        {
          "name": "order_id",
          "type": "ForeignKey",
          "target": "orders.id"
        },
        {
          "name": "product_id",
          "type": "ForeignKey",
          "target": "products.id"
        }
      ],
      "relationships": [
        {
          "target_table": "orders",
          "target_class": "Order",
          "type": "many_to_one",
          "back_populates": "order_items"
        },
        {
          "target_table": "products",
          "target_class": "Product",
          "type": "many_to_one",
          "back_populates": "order_items"
        }
      ]
    }
  ]
}
